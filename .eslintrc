{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "import",
    "jsx-a11y"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "impliedStrict": true
    }
  },
  "env": {
    "jest": true,
    "browser": true
  },
  "globals": {
    "__DEV__": true
  }
}
